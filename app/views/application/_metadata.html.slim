meta charset="utf-8"
title = view_title(@title)
meta name="description" content=view_description(@description)
meta name="keywords" content=view_keywords(@keywords)
meta name="author" content=view_author(@author)
meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"
meta name="viewport" content="width=device-width"
